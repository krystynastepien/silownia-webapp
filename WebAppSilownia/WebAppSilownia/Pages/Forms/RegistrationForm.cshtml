@page
@model WebAppSilownia.RegistrationFormModel
@{
    ViewData["Title"] = "Register";
}

<h1>Register</h1>

<form method="post">
    <input type="text" asp-for="Register.FirstName" placeholder="First Name " /><br /><br />
    <input type="text" asp-for="Register.LastName" placeholder="Last Name " /><br /><br />
    <input type="text" asp-for="Register.Email" placeholder="Email " /><br /><br />
    <input type="text" asp-for="Register.Username" placeholder="Username " /><br /><br />
    <input type="text" asp-for="Register.Password" placeholder="Password " /><br /><br />
    <input type="text" asp-for="Register.RepeatPassword" placeholder="Repeat Password " /><br /><br />
    <button type="submit">Submit</button><button type="reset">Reset</button>
</form>